{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext"],
    "jsx": "react-native",
    "moduleResolution": "bundler",
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "strict": false,
    "types": ["nativewind/types"]
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}